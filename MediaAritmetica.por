programa {
  funcao inicio() {
    real nota1,nota2,nota3,nota4,media

    escreva("Entrada da Nota 1:")
    leia(nota1)
    escreva("Entrada Nota 2:")
    leia(nota2)
    escreva("Entrada da Nota 3:")
    leia(nota3)
    escreva("Entrada da Nota 4:")
